			Mass="10"
			AngularDamping="0.8" 
			ModelFrame="BoneBranch"
			DamageMult="0.0"
		>
			<Constraint Cone="90" _template="BranchSoft">
				<PlaneConeMotor  Damping="1500" Spring="10000" Type="Spring" />
			</Constraint>
		</wood_blocker_us16_branch>
	</Body>
</_templates>
